@model IEnumerable<TDistrict>
<div class="nav-item dropdown">
    <a href="#" class="nav-link dropdown-toggle" data-bs-toggle="dropdown">Districts</a>
    <div class="dropdown-menu m-0">
        @foreach (var item in Model)
        {
            <a asp-controller="Home" asp-action="DistrictDetail" asp-route-districtID="@item.DistrictId" class="dropdown-item">@item.Name</a>
        }
    </div>
</div>